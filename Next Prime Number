#include <iostream>
#include <math.h>  

int Isprime(int n)
{
    int i = 3;
    
    int max = sqrt(n);
    
    if(n % 2 ==0){
        return 0;
    }
    
    while (i <=  max + 1){
        if(n % i ==0)
        {
            return 0;
        }
        i++;
    }
    return 1;
}

int main()

{
    for( int i = 2; i <= 1000;i++)
    {
        if(Isprime(i)  ==  1)
        {
           std::cout << i << ",";
        }
    }
    return 0;
}	
